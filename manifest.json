{
    "manifest_version": 3,
    "name": "Media Search",
    "icons": {
        "64":"icons/icon32.png"
    },
    "version": "0.1",
    "background":{
        "service_worker":"serviceWorker.js",
        "type":"module"
    },
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}